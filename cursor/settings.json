{
  "editor.scrollBeyondLastLine": false,
  "[python]": {
    "editor.defaultFormatter": null,
    "editor.formatOnSave": true,
    "editor.formatOnType": true
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "tailwindCSS.suggestions": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.wordBasedSuggestions": "off",
    "editor.suggest.insertMode": "replace",
    "editor.formatOnSave": true,
    "editor.semanticHighlighting.enabled": true
  },
  "python.formatting.provider": "autopep8",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--ignore=E402, E501, W503"],
  "python.formatting.autopep8Args": [
    "--ignore",
    "E50",
    "--max-line-length",
    "120",
    "--aggressive"
  ],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.hi": "default"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "yaml.schemas": {},
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "go.toolsManagement.autoUpdate": true,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "json.schemas": [],
  "emmet.triggerExpansionOnTab": true,
  "editor.tabSize": 4,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "rust-analyzer.server.extraEnv": {
    "CHALK_OVERFLOW_DEPTH": "1000"
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[clojure]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordSeparators": "\t ()\"':,;~@#$%^&{}[]`",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingOvertype": "always",
    "editor.autoClosingQuotes": "always",
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "betterthantomorrow.calva",
    "editor.autoIndent": "full",
    "editor.matchBrackets": "never",
    "editor.guides.indentation": false,
    "editor.parameterHints.enabled": false,
    "editor.unicodeHighlight.allowedCharacters": {
      " ": true,
      "꞉": true
    },
    "editor.foldingStrategy": "indentation"
  },
  "vim.normalModeKeyBindings": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    }
  ],
  "editor.linkedEditing": true,
  "html.format.enable": true,
  "html.autoClosingTags": true,
  "html.format.indentInnerHtml": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.startupEditor": "none",
  "[xml]": {
    "editor.defaultFormatter": "mikeburgh.xml-format",
    "editor.formatOnSave": true
  },
  "lldb.launch.sourceMap": {
    "/rustc/9d1b2106e23b1abd32fce1f17267604a5102f57a": "~/.rustup/toolchains/stable-aarch64-apple-darwin/lib/rustlib/src/rust"
  },
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "~/.vimrc.vscode",
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "top",
  "editor.formatOnSave": true,
  "material-icon-theme.folders.color": "#ef5350",
  "haskell.manageHLS": "GHCup",
  "workbench.iconTheme": "vscode-icons",
  "redhat.telemetry.enabled": false,
  "vsicons.dontShowNewVersionMessage": true,
  "terminal.integrated.sendKeybindingsToShell": true,
  "[shellscript]": {
    "editor.formatOnSave": false
  },
  "hediet.vscode-drawio.resizeImages": null,
  "git.openRepositoryInParentFolders": "always",
  "markdown.marp.enableHtml": true,
  "markdown.marp.pdf.noteAnnotations": true,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "vim.useSystemClipboard": true,
  "cursor.cpp.disabledLanguages": ["plaintext", "scminput"],
  "workbench.colorTheme": "Default Dark+"
}
